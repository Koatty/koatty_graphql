    "@rollup/plugin-json": "^6.x.x",
    "@types/jest": "^29.x.x",
    "@types/koa": "^2.x.x",
    "@types/node": "^20.x.x",
    "@typescript-eslint/eslint-plugin": "^6.x.x",
    "@typescript-eslint/parser": "^6.x.x",
